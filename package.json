{
  "name": "oio",
  "version": "1.9.1",
  "main": "dist/index.js",
  "description": "A UI component kit for React",
  "scripts": {
    "build": "rm -rf dist && webpack --config config/webpack.config.js && gulp",
    "demo": "node ./demo/server",
    "dev": "npm run demo",
    "lint": "git diff HEAD --name-only --diff-filter=ACM | grep '.js$' | xargs node ./node_modules/eslint/bin/eslint.js --quiet",
    "lint-full": "node ./node_modules/eslint/bin/eslint.js .",
    "precommit": "npm run lint",
    "postmerge": "npm install",
    "postreceive": "npm install",
    "postrewrite": "npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "testone-old": "NODE_ENV=testing ./node_modules/mocha-webpack/bin/mocha-webpack -r jsdom-global/register --require @babel/polyfill --webpack-config config/webpack.test.config.js",
    "test-old": "NODE_ENV=testing ./node_modules/mocha-webpack/bin/mocha-webpack -r jsdom-global/register --require @babel/polyfill --webpack-config config/webpack.test.config.js \"tests/**/*.js\"",
    "webpack-analyze": "mkdir -p dist; NODE_ENV=production webpack --config config/webpack.config.js --profile --json > dist/stats.json; webpack-bundle-analyzer dist/stats.json"
  },
  "files": [
    "dist/index.js",
    "dist/styles.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mother/oio.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "component",
    "components",
    "kit",
    "UI",
    "material"
  ],
  "author": "Mother Co.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mother/oio/issues"
  },
  "homepage": "https://github.com/mother/oio",
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "blob": "0.0.4",
    "chai": "4.1.2",
    "classnames": "2.2.5",
    "css-loader": "0.28.11",
    "emotion": "9.1.1",
    "enzyme": "3.4.4",
    "enzyme-adapter-react-16.3": "1.1.1",
    "eslint-config-mother": "github:mother/javascript#master",
    "eslint-plugin-jest": "21.27.1",
    "express": "4.16.2",
    "form-data": "2.1.2",
    "gulp": "3.9.1",
    "gulp-concat": "2.6.0",
    "gulp-less": "4.0.0",
    "husky": "0.13.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jsdom": "12.0.0",
    "jsdom-global": "3.0.2",
    "less": "3.0.1",
    "less-loader": "4.1.0",
    "moment": "2.22.0",
    "prop-types": "15.6.0",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-files": "2.4.7",
    "react-router-dom": "4.2.2",
    "react-testing-library": "5.2.3",
    "resolve-url-loader": "2.3.0",
    "sinon": "6.1.5",
    "style-loader": "0.20.3",
    "throttle-debounce": "1.0.1",
    "webpack": "4.4.1",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.0.13",
    "webpack-dev-middleware": "3.1.0",
    "webpack-hot-middleware": "2.21.2"
  }
}
